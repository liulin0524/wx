/**app.wxss**/
/* icon */
@font-face {
  font-family: 'icomoon';
  src:  url('fonts/icomoon.eot?rqkqb4');
  src:  url('fonts/icomoon.eot?rqkqb4#iefix') format('embedded-opentype'),
    url('fonts/icomoon.ttf?rqkqb4') format('truetype'),
    url('fonts/icomoon.woff?rqkqb4') format('woff'),
    url('fonts/icomoon.svg?rqkqb4#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'icomoon' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-home:before {
  content: "\e900";
}
.icon-doc:before {
  content: "\e926";
}
.icon-user:before {
  content: "\e971";
}
.tc{
  text-align: center
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx 0;
  box-sizing: border-box;
} 
